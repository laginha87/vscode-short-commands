{
    "name": "short-commands",
    "displayName": "Short Commands",
    "description": "Alternative command palette that relies on short aliases.",
    "version": "0.0.4",
    "publisher": "laginha87",
    "icon": "images/icon.png",
    "repository": "https://github.com/laginha87/vscode-short-commands",
    "engines": {
        "vscode": "^1.26.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:short-commands.activatePalette"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "short-commands.activatePalette",
                "category": "Short Commands",
                "title": "Activate Palette"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/glob": "^5.0.35",
        "@types/mocha": "^2.2.42",
        "@types/node": "^8.10.25",
        "decache": "^4.4.0",
        "glob": "^7.1.2",
        "istanbul": "^0.4.5",
        "remap-istanbul": "^0.11.1",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.21"
    }
}
